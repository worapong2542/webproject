        <script>
            $( document ).ready(function() {
                $.getJSON("https://covid19.th-stat.com/api/open/today", function(result){
                console.log(result)
                document.getElementById("Confirmed").innerHTML = "Confirmed: "+JSON.stringify(result.Confirmed);
                document.getElementById("Recovered").innerHTML = "Recovered: "+JSON.stringify(result.Recovered);
                document.getElementById("Hospitalized").innerHTML = "Hospitalized: "+JSON.stringify(result.Hospitalized);
                document.getElementById("Deaths").innerHTML = "Deaths: "+JSON.stringify(result.Deaths);
                document.getElementById("NewConfirmed").innerHTML = "NewConfirmed: "+JSON.stringify(result.NewConfirmed);
                document.getElementById("NewRecovered").innerHTML = "NewRecovered: "+JSON.stringify(result.NewRecovered);
                document.getElementById("NewHospitalized").innerHTML = "NewHospitalized: "+JSON.stringify(result.NewHospitalized);
                document.getElementById("NewDeaths").innerHTML = "NewDeaths: "+JSON.stringify(result.NewDeaths);
                document.getElementById("UpdateDate").innerHTML = "UpdateDate: "+JSON.stringify(result.UpdateDate).slice(1,-1);
            });
            });
            
